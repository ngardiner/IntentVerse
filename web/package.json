{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "jest-junit": "^16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "JEST_JUNIT_OUTPUT_NAME=web-core-test-results.xml react-scripts test --testPathPattern='(App\\.test\\.|api/)' --watchAll=false --ci --silent --testResultsProcessor=jest-junit",
    "test:web-auth": "JEST_JUNIT_OUTPUT_NAME=web-auth-test-results.xml react-scripts test --testPathPattern=auth --watchAll=false --ci --silent --testResultsProcessor=jest-junit",
    "test:web-pages": "JEST_JUNIT_OUTPUT_NAME=web-pages-test-results.xml react-scripts test --testPathPattern=pages --watchAll=false --ci --silent --testResultsProcessor=jest-junit",
    "test:web-components": "JEST_JUNIT_OUTPUT_NAME=web-components-test-results.xml react-scripts test --testPathPattern=components --watchAll=false --ci --silent --testResultsProcessor=jest-junit",
    "test:verbose": "react-scripts test --coverage --watchAll=false --verbose",
    "test:debug": "react-scripts test --watchAll=false --verbose --no-cache",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.spec.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 75,
        "statements": 75
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "jest-junit": {
    "outputDirectory": "./test-results",
    "outputName": "test-results.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}